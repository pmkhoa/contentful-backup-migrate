  "author": "Marcos Mellado <marcos.mellado@akqa.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^3.0.1",
    "contentful-export": "^7.6.0",
    "contentful-import": "^7.9.6",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
